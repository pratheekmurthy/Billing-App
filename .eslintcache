 })}</div>\n            <div>\n                {open && (\n                    <Modal isOpen={open}>\n                    <ProductForm\n                        id={id}\n                        name={name}\n                        price={price}\n                        handleToggle={handleToggle}\n                    />\n                    <button onClick={() => {\n                        handleToggle()\n                    }} className=\"btn-danger\">close</button>\n                    </Modal>\n                )}\n                </div>\n        </>\n        \n    </div>)\n}\n\nexport default ProductItem\n\n\n{/* <tr key={product._id}>\n                    <td> {product.name} -{product.price} </td>\n                    <td><button onClick={() => {\n                        dispatch(startGetProd(product._id))\n                    }}><VisibilityIcon/></button>\n                    <button onClick={() => {\n                        handleEdit(product)\n                    }}><EditIcon/></button>\n                   <button onClick={() => {\n                        deleteProduct(product._id)\n                    }}><DeleteIcon/></button></td>\n                </tr> */}","/home/pratheek/Desktop/React/Redux/billing-app/src/reducers/productsReducer.js",[],"/home/pratheek/Desktop/React/Redux/billing-app/src/styling/app-styled.js",[],"/home/pratheek/Desktop/React/Redux/billing-app/src/actions/billAction.js",[],"/home/pratheek/Desktop/React/Redux/billing-app/src/components/Billoperations/Lineitems.js",[],"/home/pratheek/Desktop/React/Redux/billing-app/src/components/Billoperations/Billgenerator.js",["144"],"/home/pratheek/Desktop/React/Redux/billing-app/src/reducers/billReducer.js",[],"/home/pratheek/Desktop/React/Redux/billing-app/src/components/Billoperations/BillDetails.js",[],"/home/pratheek/Desktop/React/Redux/billing-app/src/components/Billoperations/Showaddedproduct.js",[],"/home/pratheek/Desktop/React/Redux/billing-app/src/components/Billoperations/AllBill.js",[],"/ho